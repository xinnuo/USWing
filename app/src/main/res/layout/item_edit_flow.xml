<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/item_flow_title"
        style="@style/Font13_black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dimen_size_8"
        android:layout_marginEnd="@dimen/dimen_size_8"
        android:background="@drawable/rec_bg_trans_stroke_black_r0"
        android:paddingStart="@dimen/dimen_size_15"
        android:paddingTop="@dimen/dimen_size_10"
        android:paddingEnd="@dimen/dimen_size_15"
        android:paddingBottom="@dimen/dimen_size_10"
        android:text="标题" />

    <ImageView
        android:id="@+id/item_flow_del"
        android:layout_width="@dimen/dimen_size_16"
        android:layout_height="@dimen/dimen_size_16"
        android:layout_gravity="end"
        android:src="@mipmap/video_icon10"
        tools:ignore="ContentDescription" />

</FrameLayout>